<h4>Classes</h4>
<table class="table table-striped table-hover" id="entrytable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Package</th>
      <th>Updated At</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% @project.klasses.each do |klass| %>
      <tr data-link="<%= klass_path(@project, klass) %>">
        <td><%= klass.name %></td>
        <td><%= klass.package%></td>
        <td><%= klass.updated_at.strftime("%d-%m-%Y") %></td>
        <td><%= klass.created_at.strftime("%d-%m-%Y") %></td>
      </tr>
    <% end %>
  </tbody>
</table>
