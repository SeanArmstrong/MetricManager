<p id="notice"><%= notice %></p>

<h3>Project Profile: <%= @project.name %></h3>

<table class="table table-striped table-hover" id="entrytable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Package</th>
      <th>Updated At</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% @project.klasses.each do |klass| %>
      <tr data-link="<%= klass_path(@project, klass) %>">
        <td><%= klass.name %></td>
        <td><%= klass.package%></td>
        <td><%= klass.updated_at %></td>
        <td><%= klass.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
