<h1>Listing methoods</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @methoods.each do |method| %>
      <tr>
        <td><%= method.name %></td>
        <td><%= link_to 'Show', method_path(method.klass.project.id, method.klass.id, method.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
