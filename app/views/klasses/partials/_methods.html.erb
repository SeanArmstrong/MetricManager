<h4>Methods</h4>
<table class="table table-striped table-hover" id="entrytable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Updated At</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% @klass.methoods.each do |method| %>
      <tr data-link="<%= method_path(@klass.project, @klass.id, method.id) %>">
        <td><%= method.name %></td>
        <td><%= method.updated_at %></td>
        <td><%= method.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
