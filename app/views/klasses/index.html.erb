<h1>Listing klasses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @klasses.each do |klass| %>
      <% klass %>
      <tr>
        <td><%= klass.name %></td>
        <td><%= link_to 'Show', klass_path(klass.project.id, klass.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
