<p id="notice"><%= notice %></p>

<h3>Class Profile: <%= @klass.name %></h3>
<% if @klass.klass_result_sets.any? %>

  <%= render "klasses/partials/latest_figures.html.erb" %>
  <%= render "klasses/partials/history.html.erb" %>

  <h4>Methods</h4>
  <table class="table table-striped table-hover" id="entrytable">
    <thead>
      <tr>
        <th>Name</th>
        <th>Updated At</th>
        <th>Created At</th>
      </tr>
    </thead>
    <tbody>
      <% @klass.methoods.each do |method| %>
        <tr data-link="<%= method_path(@klass.project, @klass.id, method.id) %>">
          <td><%= method.name %></td>
          <td><%= method.updated_at %></td>
          <td><%= method.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>

<% else %>
Nope

<% end %>
<%= link_to 'Back', klasses_path %>
